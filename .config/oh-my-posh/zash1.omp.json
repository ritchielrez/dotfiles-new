{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#E36464",
          "style": "plain",
          "template": "@{{ .UserName }}",
          "type": "session"
        },
        {
          "foreground": "#7daea3",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": " {{ .Path }}",
          "type": "path"
        },
        {
          "foreground": "#a9b665",
          "properties": {
            "branch_icon": ""
          },
          "style": "plain",
          "template": " \ue725 {{ .HEAD }}",
          "type": "git"
        },
        {
          "foreground": "#8ec07c",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ea6962{{ end }}"
          ],
          "leading_diamond": " ",
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "template": "\uf0a9 ",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
