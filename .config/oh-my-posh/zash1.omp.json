{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#E36464",
          "properties": {
            "template": "@{{ .UserName }}"
          },
          "style": "plain",
          "type": "session"
        },
        {
          "foreground": "#56B6C2",
          "properties": {
            "style": "folder",
            "template": " {{ .Path }}"
          },
          "style": "plain",
          "type": "path"
        },
        {
          "foreground": "#b8bb26",
          "properties": {
            "branch_icon": "",
            "template": " \ue725 {{ .HEAD }}"
          },
          "style": "plain",
          "type": "git"
        },
	{
		"type": "exit",
		"style": "diamond",
		"foreground": "#8ec07c",
		"foreground_templates": [
			"{{ if gt .Code 0 }}#fb4934{{ end }}"
		],
		"leading_diamond": " ",
		"trailing_diamond": "",
		"properties": {
			"always_enabled": true,
			"template": "\uf0a9 "
		}
	}
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 1
}
